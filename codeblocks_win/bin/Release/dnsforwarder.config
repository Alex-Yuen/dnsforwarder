# DNS 转发器的配置文件
# Configure File for dnsforwarder

Debug true

# LocalInterface <IP>
# 设置在本地开启的界面，可以是本地回环地址 (127.0.0.1) ，本地局域网，以及互联网
LocalInterface 127.0.0.1

# LocalPort <NUM>
# 本地开启的端口
LocalPort 53

# OpenLocalTCP <BOOLEAN>
# 在本地开启TCP，以便接受TCP协议的查询
# 可选值：`false' 或 `true'
OpenLocalTCP false


# 您可以设置两个上游服务器，
# 一个是用 TCP 协议的服务器 (由 `TCPServer' 指定)，
# 另一个是用 UDP 协议的服务器 (由 `UDPServer' 指定)

# 选项 `PrimaryServer' 用来指定首选的服务器，
# 可以是 `TCP' (用 TCP 协议的服务器) 或者 `UDP' (用 UDP 协议的服务器)
# 任何不在排除列表中的域都会通过首选服务器来查询，
# 在排除列表中的域会使用另一个服务器查询

# PrimaryServer 
# 首选服务器
# 可选值：`TCP' 或 `UDP'
PrimaryServer UDP

# TCPServer <IP[:PORT]>
# 指定用 TCP 协议查询的上游服务器
TCPServer 8.8.4.4,8.8.8.8

# UDPServer <IP[:POTR]>
# 指定用 UDP 协议查询的上游服务器
UDPServer 202.99.216.113,202.97.131.178

FallBackToSecondary true

TimeToServer 3000

# ExcludedDomain <STRING>
# 排除列表，每个域之间用半角逗号 (`,') 分隔，同时注意最后不要有逗号
# 可以有多条ExcludedDomain
ExcludedDomain *google*,*proxy*,thawte.com,*wiki*

GfwList https://autoproxy-gfwlist.googlecode.com/svn/trunk/gfwlist.txt

GfwListBase64Decode true

GfwListFlushTime 7200

# UDPThreads <NUM>
# 在本地进行 UDP 监听的线程数
UDPThreads 3

# DisabledType <STRING>
# 禁止查询的 DNS 类型列表，每个类型之间用半角逗号 (`,') 分隔，同时注意最后不要有逗号
# 所有的 DNS 类型：
# A	          1   IPv4 Address
# AAAA       28   IPv6 Address
# APL        42
# CERT       37
# CNAME       5
# DHCID      49
# DLV     32769
# DNAME      39
# DNSKEY     48
# DS	     43
# HIP        55
# IPSECKEY   45
# KEY        25
# KX	     36
# LOC        29
# MX	     15
# NAPTR      35
# NS	      2   Name Server
# NSEC       47
# NSEC3      50
# NSEC3PARAM 51
# PTR        12   Domain pointer
# RRSIG      46
# RP	     17
# SIG        24
# SOA         6   start of authority record
# SPF        99
# SRV        33
# SSHFP      44
# TA	  32768
# TKEY      249
# TSIG      250
# TXT        16
# ANY       255
# AXFR      252
# IXFR      251
# OPT        41
DisabledType 28

# DisabledDomain <STRING>
# 禁止查询的域列表，每个域之间用半角逗号 (`,') 分隔，同时注意最后不要有逗号
DisabledDomain *dnserror*.wo.com.cn,.googleadservices.com,.doubleclick.net,wqzyt.net,cpro.baidu.com,drmcmm.baidu.com,eiv.baidu.com,cnzz.com,gdt.qq.com,pgdt.gtimg.cn,ishop.baidu.com,0355114.org,netloader.cc,kd0351.com,njzzw.com,z.alimama.com,exoclick.com,weipage.cn

# UseCache <BOOLEAN>
# 是否使用缓存 (文件缓存)
# 可选值：`false' 或 `true'
UseCache true

# CacheSize <NUM>
# 缓存大小 (字节)
CacheSize 1048576

# IgnoreTTL <BOOLEAN>
# 是否忽略 TTL
# 可选值：`false' 或 `true'
# 为 `true' 时将忽略 TTL，此时所有的缓存条目将不会被移除
# 为 `false' 时不忽略 TTL，缓存条目将会按照 TTL 的时间来移除
# 当 `UseCache' 的值为 `false' 时，此选项无效
IgnoreTTL false

# ForceTTL <NUM>
# 强制使所有缓存的条目的 TTL 为 <NUM>
# 若 <NUM> 为 -1，则表示不进行强制
# 当 `UseCache' 的值为 `false' 时，此选项无效
# 当 `IgnoreTTL' 的值为 `true' 时，此选项无效
ForceTTL -1

# MultipleTTL <NUM>
# 将所有缓存条目的 TTL 加倍，倍数为 <NUM>
# 当 `UseCache' 的值为 `false' 时，此选项无效
# 当 `IgnoreTTL' 的值为 `true' 时，此选项无效
# 当 `ForceTTL' 的值不为 -1 时，此选项无效
MultipleTTL 10

ReloadCache true

OverwriteCache true

Hosts https://smarthosts.googlecode.com/svn/trunk/hosts

#Hosts https://raw.github.com/orzFly/HostsX/master/hosts

#Hosts https://huhamhire-hosts.googlecode.com/git/downloads/raw/ipv4_win_ansi/hosts

#Hosts http://openggme/wp-content/uploads/2011/09/hosts.php?4

#Hosts /etc/hosts

HostsFlushTime 1800

HostsDownloadPath 

HostsScript 

AppendHosts www.google.cn *.googlecode.com
AppendHosts www.google.cn *.gstatic.com
AppendHosts www.google.cn mail.l.google.com